uint8_t readmsg(void ){
  uint8_t count=0, i=0; 
  uint8_t lenmsg=0;
  uint8_t readbyte;
  uint16_t u16Aux;
  float fAux;
  uint8_t *pu8Aux;
  uint8_t pos=0;

  //read header
  readbyte = Serial2.read();
  if (readbyte == HEADER_MSG){
      pos++;
      //Read Voltage Turbidez
      pu8Aux = (uint8_t *)&fAux;
      *pu8Aux++ = Serial2.read();
      *pu8Aux++ = Serial2.read();
      *pu8Aux++ = Serial2.read();
      *pu8Aux++ = Serial2.read();
      inputframe.turb_v = fAux;
      pos+=4;
      //Read NTU Turbidez
      pu8Aux = (uint8_t *)&fAux;
      *pu8Aux++ = Serial2.read();
      *pu8Aux++ = Serial2.read();
      *pu8Aux++ = Serial2.read();
      *pu8Aux++ = Serial2.read();
      inputframe.turb_ntu = fAux;
      pos+=4;
      //read temperature sensor
      pu8Aux = (uint8_t *)&fAux;
      *pu8Aux++ = Serial2.read();
      *pu8Aux++ = Serial2.read();
      *pu8Aux++ = Serial2.read();
      *pu8Aux++ = Serial2.read();
      inputframe.temp = fAux;
      pos+=4;
      //read voltage condutivity
      pu8Aux = (uint8_t *)&fAux;
      *pu8Aux++ = Serial2.read();
      *pu8Aux++ = Serial2.read();
      *pu8Aux++ = Serial2.read();
      *pu8Aux++ = Serial2.read();
      inputframe.cond_v = fAux;
      pos+=4;
      //read tds condutivity
      pu8Aux = (uint8_t *)&fAux;
      *pu8Aux++ = Serial2.read();
      *pu8Aux++ = Serial2.read();
      *pu8Aux++ = Serial2.read();
      *pu8Aux++ = Serial2.read();
      inputframe.cond_tds = fAux;
      pos+=4;
      //read footer
      pu8Aux = (uint8_t *)&u16Aux;
      *pu8Aux++ = Serial2.read();
      *pu8Aux++ = Serial2.read();
      inputframe.footer = u16Aux;
      pos+=2;      

      if (inputframe.footer == FOOTER_MSG)
        return pos;
      else
        Serial.println("error RX message!!!");
  }
  return 0;
}


void readloop(){

   float turb_qual;
   float cond_qual;
   float ang,aux2;

    while (Serial2.available()) {
      
      if ( readmsg() > 0){
       
        //Water Quality Indicator in Percentage
        ang = (float) (-100.0)/(TURB_NTU_MAX - TURB_NTU_MIN);
        turb_qual = ang * (inputframe.turb_ntu - TURB_NTU_MAX);
        if (turb_qual >100)
            turb_qual =100.0;
        if (turb_qual < 0.0)
            turb_qual = 0.0;    
        ang = (float) (-100.0)/(TURB_TDS_MAX - TURB_TDS_MIN);
        cond_qual = ang * (inputframe.cond_tds - TURB_TDS_MAX);
        if (cond_qual > 100)
            cond_qual = 100.0;
        if (cond_qual < 0.0)
            cond_qual = 0.0;    
        
        fwaterquality = (turb_qual + cond_qual)/2;

        #if IMPRIME_SERIAL
          Serial.print("Turbidez: V= ");
          Serial.print(inputframe.turb_v);
          Serial.print(" NTU= ");
          Serial.print(inputframe.turb_ntu);
          Serial.print(" - Condutividade: V= ");
          Serial.print(inputframe.cond_v);
          Serial.print(" Tds= ");
          Serial.print(inputframe.cond_tds);
          Serial.print(" - Temperatura =");
          Serial.print(inputframe.temp);
          Serial.print(" - Qualidade ");
          Serial.print(" turb =");
          Serial.print(turb_qual);
          Serial.print(" cond =");
          Serial.print(cond_qual);
          Serial.print(" média_qual =");
          Serial.println(fwaterquality);
        #endif 

        board["turb"] = inputframe.turb_ntu;
        board["temp"] = inputframe.temp;
        board["cond"] = inputframe.cond_tds;
        board["qual"] = fwaterquality;
        board["qualtu"] = turb_qual;
        board["qualco"] = cond_qual;

        String jsonString = JSON.stringify(board);
        events.send(jsonString.c_str(), "new_readings", millis());       
        count++;       
      } 
       
  }
}


void TaskSerialManager ( void * pvParameters ){
 
  for(;;){
    if (blinkvalue)
      blinkvalue = 0;
    else
      blinkvalue = 1;
    
    digitalWrite(LED_BUILTIN, blinkvalue); 

    readloop();

    delay(1000);
  }
  
  vTaskDelete( NULL );
  
}
 
